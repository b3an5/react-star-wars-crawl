{
  "name": "react-star-wars-crawl",
  "version": "1.0.0",
  "description": "A composable component that recreates the Star Wars opening crawl",
  "main": "./lib/index.js",
  "jsnext:main": "./es/index.js",
  "scripts": {
    "test": "test",
    "build": "npm run build:lib && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.js dist/react-star-wars-crawl.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js dist/react-star-wars-crawl.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdubrock/react-star-wars-crawl.git"
  },
  "keywords": [
    "react",
    "star",
    "wars",
    "crawl"
  ],
  "author": "Russ DuBrock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rdubrock/react-star-wars-crawl/issues"
  },
  "homepage": "https://github.com/rdubrock/react-star-wars-crawl#readme",
  "dependencies": {
    "react": "^15.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.1"
  }
}
